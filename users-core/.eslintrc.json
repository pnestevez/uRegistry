{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "arrow-parens": "off",
        "no-unused-expressions": "off",
        "no-return-assign": "off",
        "no-param-reassign": "off",
        "consistent-return": "off",
        "no-return-await": "off",
        "no-console": "off"
    }
}